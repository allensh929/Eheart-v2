<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="com.eheart.domain.User" uses-template="simple"/>

    <cache alias="com.eheart.domain.Authority" uses-template="simple"/>

    <cache alias="com.eheart.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.eheart.domain.PersistentToken" uses-template="simple"/>

    <cache alias="com.eheart.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="com.eheart.domain.ProductCategory" uses-template="simple"/>

    <cache alias="com.eheart.domain.ProductCategory.hasProducts" uses-template="simple"/>

    <cache alias="com.eheart.domain.Product" uses-template="simple"/>

    <cache alias="com.eheart.domain.Product.myCategorys" uses-template="simple"/>

    <cache alias="com.eheart.domain.Eheart" uses-template="simple"/>

    <cache alias="com.eheart.domain.Menu" uses-template="simple"/>

    <cache alias="com.eheart.domain.Menu.hasSubMenus" uses-template="simple"/>

    <cache alias="com.eheart.domain.SubMenu" uses-template="simple"/>

    <cache alias="com.eheart.domain.SubMenu.hasSubMenus" uses-template="simple"/>

    <cache alias="com.eheart.domain.ThirdMenu" uses-template="simple"/>

    <cache alias="com.eheart.domain.Domain" uses-template="simple"/>

    <cache alias="com.eheart.domain.Domain.doctors" uses-template="simple"/>

    <cache alias="com.eheart.domain.Doctor" uses-template="simple"/>

    <cache alias="com.eheart.domain.Doctor.domains" uses-template="simple"/>

    <cache alias="com.eheart.domain.Title" uses-template="simple"/>

    <cache alias="com.eheart.domain.Hospital" uses-template="simple"/>

    <cache alias="com.eheart.domain.Hospital.departments" uses-template="simple"/>

    <cache alias="com.eheart.domain.Department" uses-template="simple"/>

    <cache alias="com.eheart.domain.Department.hospitals" uses-template="simple"/>

    <cache alias="com.eheart.domain.ProductCategory.hasSubCategories" uses-template="simple"/>

    <cache alias="com.eheart.domain.Hospital.clinics" uses-template="simple"/>

    <cache alias="com.eheart.domain.Clinic" uses-template="simple"/>

    <cache alias="com.eheart.domain.Clinic.departments" uses-template="simple"/>

    <cache alias="com.eheart.domain.Clinic.hospitals" uses-template="simple"/>

    <cache alias="com.eheart.domain.ProductSubCategory" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
